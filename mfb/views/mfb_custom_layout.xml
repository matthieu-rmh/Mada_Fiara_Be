<odoo>
    <data>
        <template id="mfb_custom_layout" inherit_id="web.external_layout_standard">
            <!-- <link href="https://fonts.googleapis.com/css?family=Dancing+Script" rel="stylesheet"/> -->
            <xpath expr="//div[1]" position="replace">
                <div t-attf-class="header o_company_#{company.id}_layout" t-att-style="report_header_style">
                    <div>
                        <div class="row">
                            <div class="col-4">
                            <p><h2 class="text-center" style="font-size: 25px;font-family: 'Tinos-Bold';"><span t-esc="company.name.upper()"/></h2></p>
                            <p><h2 class="text-center" style="font-size: 25px;font-family: 'Tinos-Bold';"><span t-esc="company.subname.upper()"/></h2></p>
                            <p><h2 class="text-center" style="font-size: 20px;"><span t-esc="company.partner_id.street.upper()"/></h2></p>
                            <p><h2 class="text-center" style="font-size: 20px;"><span t-esc="company.partner_id.city.upper()"/> <span t-esc="company.partner_id.state_id.name.upper()"/></h2></p>
                            </div>
                            <div class="col-4"></div>
                            <div class="col-4"></div>
                        </div>
                    </div>
                    
                </div>
            </xpath>
        </template>
    </data>
</odoo>